grammar com.xkp.codexcess.Xss with org.eclipse.xtext.common.Terminals

generate xss "http://www.xkp.com/codexcess/Xss"

Program
	:	block=Block
	;

Block
	:	'{' StmList? '}'
	;

StmList
	:	Statement+
	;

Statement
	:	LocalVarDecl ';'
	|	Block
	;

LocalVarDecl
	:	'TYPE' VariableDeclarator
	;

VariableDeclarator
	:	ID 
	;


